
#include <iostream>

using namespace std;

int* compareTriplets(int a[3], int b[3]){
    int a_points=0, b_points=0;
    for(int i=0;i < 3;i++){
    if(a[i] > b[i]){
        a_points++;
    }else if(b[i] > a[i]){
        b_points++;
    }
    }

    static int  arr[2];
    arr[0] = a_points;
    arr[1] = b_points;
    return arr;
}

int main(){
    int a[3], b[3];
    cin >> a[0] >> a[1] >> a[2] >> b[0] >> b[1] >> b[2];
    
    int* arrY = compareTriplets(a, b);
    cout << arrY[0] << " " << arrY[1];
    return 0;
}
